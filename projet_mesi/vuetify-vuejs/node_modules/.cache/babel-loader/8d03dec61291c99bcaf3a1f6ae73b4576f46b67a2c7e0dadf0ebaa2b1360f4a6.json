{"ast":null,"code":"import Layout from './components/Layout/Layout.vue';\nexport default {\n  data: () => ({\n    cards: ['Today', 'Yesterday'],\n    drawer: null,\n    links: [['mdi-inbox-arrow-down', 'Inbox'], ['mdi-send', 'Send'], ['mdi-delete', 'Trash'], ['mdi-alert-octagon', 'Spam']]\n  })\n};","map":{"version":3,"mappings":"AAsEA;AACA;EACAA;IACAC;IACAC;IACAC,QACA,mCACA,sBACA,yBACA;EAEA;AACA","names":["data","cards","drawer","links"],"sourceRoot":"src/views/mesiExample","sources":["ExampleMesiView.vue"],"sourcesContent":["<template>\r\n  <v-app id=\"inspire\">\r\n    <Layout>\r\n    <v-system-bar app>\r\n      <v-spacer></v-spacer>\r\n\r\n      <v-icon>mdi-square</v-icon>\r\n\r\n      <v-icon>mdi-circle</v-icon>\r\n\r\n      <v-icon>mdi-triangle</v-icon>\r\n    </v-system-bar>\r\n\r\n    <v-navigation-drawer v-model=\"drawer\" app>\r\n      <v-sheet color=\"grey lighten-4\" class=\"pa-4\">\r\n        <v-avatar class=\"mb-4\" color=\"grey darken-1\" size=\"64\"></v-avatar>\r\n\r\n        <div>john@vuetifyjs.com</div>\r\n      </v-sheet>\r\n\r\n      <v-divider></v-divider>\r\n\r\n      <v-list>\r\n        <v-list-item v-for=\"[icon, text] in links\" :key=\"icon\" link>\r\n          <v-list-item-icon>\r\n            <v-icon>{{ icon }}</v-icon>\r\n          </v-list-item-icon>\r\n\r\n          <v-list-item-content>\r\n            <v-list-item-title>{{ text }}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n\r\n    <v-main>\r\n      <v-container class=\"py-8 px-6\" fluid>\r\n        <v-row>\r\n          <v-col v-for=\"card in cards\" :key=\"card\" cols=\"12\">\r\n            <v-card>\r\n              <v-subheader>{{ card }}</v-subheader>\r\n\r\n              <v-list two-line>\r\n                <template v-for=\"n in 6\">\r\n                  <v-list-item :key=\"n\">\r\n                    <v-list-item-avatar color=\"grey darken-1\">\r\n                    </v-list-item-avatar>\r\n\r\n                    <v-list-item-content>\r\n                      <v-list-item-title>Message {{ n }}</v-list-item-title>\r\n\r\n                      <v-list-item-subtitle>\r\n                        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nihil repellendus distinctio similique\r\n                      </v-list-item-subtitle>\r\n                    </v-list-item-content>\r\n                  </v-list-item>\r\n\r\n                  <v-divider v-if=\"n !== 6\" :key=\"`divider-${n}`\" inset></v-divider>\r\n                </template>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-main>\r\n  </Layout>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Layout from './components/Layout/Layout.vue'\r\nexport default {\r\n  data: () => ({\r\n    cards: ['Today', 'Yesterday'],\r\n    drawer: null,\r\n    links: [\r\n      ['mdi-inbox-arrow-down', 'Inbox'],\r\n      ['mdi-send', 'Send'],\r\n      ['mdi-delete', 'Trash'],\r\n      ['mdi-alert-octagon', 'Spam'],\r\n    ],\r\n  }),\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}